  "dependencies": {
    "@contentful/rich-text-html-renderer": "^13.4.0",
    "contentful": "^5.1.3",
    "html-react-parser": "^0.9.1",
    "node-sass-chokidar": "^1.3.5",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-helmet": "^5.2.0",
